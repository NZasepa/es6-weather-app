  "main": "app/src/main.js",
  "scripts": {
    "setup": "yarn install",
    "lint": "eslint .",
    "start": "http-server dist",
    "dev": "webpack-dev-server --env.dev --hot",
    "build": "webpack --env.prod -p",
    "build:dev": "webpack --env.dev",
    "test": "karma start",
    "watch:test": "yarn test -- --auto-watch --no-single-run",
    "debug": "node --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "yarn run debug -- --env.dev",
    "debug:prod": "yarn run debug -- --env.prod"
  },
